#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long long int  n,m;
	    cin>>n>>m;
	    long long int sum=0;
	     long long int arr[m];
	    for(int i=0;i<m;i++){
	        cin>>arr[i];
	    }
	    for(int i=0;i<m;i++){
	        sum+=arr[i];
	    }
	    long long k=(n*(n+1))/2;
	    long long j=k-sum;
	    cout<<j<<endl;
	}
	return 0;
}
